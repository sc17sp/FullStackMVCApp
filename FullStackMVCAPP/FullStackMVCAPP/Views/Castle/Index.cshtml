@model IEnumerable<FullStackMVCAPP.Models.Castle>

@{
    ViewBag.Title = "Castles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Castles</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
    </tr>
}

</table>
